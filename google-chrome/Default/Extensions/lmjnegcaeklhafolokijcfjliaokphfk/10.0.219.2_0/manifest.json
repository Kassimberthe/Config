{
   "action": {
      "default_icon": {
         "128": "bitmaps/logo-128-color.png"
      },
      "default_title": "Video DownloadHelper"
   },
   "author": "Video DownloadHelper Team",
   "background": {
      "service_worker": "service/main.js",
      "type": "module"
   },
   "commands": {
      "_execute_action": {
         "suggested_key": {
            "default": "Alt+Up",
            "mac": "Ctrl+Shift+Up"
         }
      },
      "default-action": {
         "description": "Execute default action",
         "suggested_key": {
            "default": "Alt+Down",
            "mac": "Ctrl+Shift+Down"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "injected/youtube.js" ],
      "matches": [ "*://*.youtube.com/*" ],
      "run_at": "document_end",
      "world": "ISOLATED"
   }, {
      "all_frames": true,
      "js": [ "injected/youtube_untrusted.js" ],
      "matches": [ "*://*.youtube.com/*" ],
      "run_at": "document_end",
      "world": "MAIN"
   }, {
      "all_frames": true,
      "js": [ "injected/vimeo.js" ],
      "matches": [ "*://*.vimeo.com/*" ],
      "run_at": "document_end",
      "world": "ISOLATED"
   }, {
      "all_frames": true,
      "js": [ "injected/vimeo_untrusted.js" ],
      "matches": [ "*://*.vimeo.com/*" ],
      "run_at": "document_end",
      "world": "MAIN"
   }, {
      "all_frames": true,
      "js": [ "injected/instagram.js" ],
      "matches": [ "*://*.instagram.com/*" ],
      "run_at": "document_end",
      "world": "ISOLATED"
   }, {
      "all_frames": true,
      "js": [ "injected/instagram_untrusted.js" ],
      "matches": [ "*://*.instagram.com/*" ],
      "run_at": "document_end",
      "world": "MAIN"
   }, {
      "js": [ "injected/ok.js" ],
      "matches": [ "*://*.ok.ru/*" ],
      "world": "ISOLATED"
   }, {
      "all_frames": true,
      "js": [ "injected/vk.js" ],
      "matches": [ "*://vk.com/*", "*://vk.ru/*", "*://vkvideo.ru/*" ],
      "run_at": "document_end",
      "world": "ISOLATED"
   }, {
      "all_frames": true,
      "js": [ "injected/canva.js" ],
      "matches": [ "*://*.canva.com/*" ],
      "run_at": "document_end",
      "world": "ISOLATED"
   }, {
      "all_frames": true,
      "js": [ "injected/iq.js" ],
      "matches": [ "*://*.iq.com/*" ],
      "run_at": "document_end",
      "world": "ISOLATED"
   }, {
      "all_frames": true,
      "js": [ "injected/iq_untrusted.js" ],
      "matches": [ "*://*.iq.com/*" ],
      "run_at": "document_end",
      "world": "MAIN"
   }, {
      "js": [ "injected/activate.js" ],
      "matches": [ "https://v10.downloadhelper.net/activate*", "https://app.v10.downloadhelper.net/activate*" ],
      "run_at": "document_end",
      "world": "ISOLATED"
   }, {
      "js": [ "injected/changelog.js" ],
      "matches": [ "https://v10.downloadhelper.net/changelog*", "https://app.v10.downloadhelper.net/changelog*" ],
      "run_at": "document_end",
      "world": "ISOLATED"
   } ],
   "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'"
   },
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "homepage_url": "https://downloadhelper.net",
   "host_permissions": [ "\u003Call_urls>" ],
   "icons": {
      "128": "bitmaps/logo-128-color.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Video DownloadHelper",
   "optional_permissions": [ "browsingData", "downloads.open" ],
   "permissions": [ "tabs", "offscreen", "downloads", "sidePanel", "webRequest", "webNavigation", "scripting", "declarativeNetRequest", "storage", "notifications", "contextMenus" ],
   "side_panel": {
      "default_icon": {
         "128": "bitmaps/logo-128-color.png"
      },
      "default_path": "/content/sidebar.html",
      "default_title": "Video DownloadHelper"
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "10.0.219.2"
}
